.tabs .tabs-title {
  display: flex;
}
.tabs .tabs-title .title-item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 4rpx solid #fff;
  padding: 15rpx 0;
}
.tabs .tabs-title .title-item.active {
  color: var(--themeColor);
  border-bottom: 5rpx solid var(--themeColor);
}
