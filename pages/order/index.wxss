tabs block .order-main .order-item {
  color: #666;
  padding: 20rpx;
  border-bottom: 1rpx solid #ccc;
}
tabs block .order-main .order-item .order-no-row {
  display: flex;
  justify-content: space-between;
  padding: 10rpx 0;
}
tabs block .order-main .order-item .order-price-row {
  display: flex;
  justify-content: space-between;
  padding: 10rpx 0;
}
tabs block .order-main .order-item .order-price-row .order-no-value {
  color: var(--themeColor);
  font-size: 32rpx;
}
tabs block .order-main .order-item .order-time-row {
  display: flex;
  justify-content: space-between;
  padding: 10rpx 0;
}
